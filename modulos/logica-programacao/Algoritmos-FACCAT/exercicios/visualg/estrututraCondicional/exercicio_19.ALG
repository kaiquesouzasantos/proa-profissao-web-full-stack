ALGORITMO "exercicio_19"

VAR
   valorA, valorB : inteiro

INICIO
   ESCREVAL("DIGITE O 1 VALOR: ")
   LEIA(valorA)

   ESCREVAL("DIGITE O 2 VALOR: ")
   LEIA(valorB)
   
   SE valorA > valorB ENTAO
      ESCREVAL("MAIOR VALOR: " , valorA)
   SENAO
      ESCREVAL("MAIOR VALOR: " , valorB)
   FIMSE
   
FIMALGORITMO