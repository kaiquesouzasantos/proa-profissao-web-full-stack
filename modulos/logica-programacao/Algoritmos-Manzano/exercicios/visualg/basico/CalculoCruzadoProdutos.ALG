ALGORITMO "CalculoCruzadoProdutos"
VAR
    valorA, valorB, valorC, valorD: INTEIRO

INICIO
    ESCREVA("DIGITE O PRIMEIRO VALOR: ")
    LEIA(valorA)

    ESCREVA("DIGITE O SEGUNDO VALOR: ")
    LEIA(valorB)

    ESCREVA("DIGITE O TERCEIRO VALOR: ")
    LEIA(valorC)

    ESCREVA("DIGITE O QUARTO VALOR: ")
    LEIA(valorD)

    ESCREVA("RESULTANTE DO PRODUTO DO 1 COM O 3 VALOR: ", (valorA * valorC), " | RESULTANTE DA SOMA DO 2 COM O 4 VALOR: ", (valorB + valorD))
FIMALGORITMO