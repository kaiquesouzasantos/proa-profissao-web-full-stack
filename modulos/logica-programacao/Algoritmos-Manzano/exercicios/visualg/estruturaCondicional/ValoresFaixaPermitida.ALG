ALGORITMO "ValoresFaixaPermitida"
VAR
    numero: REAL

INICIO
    ESCREVA("DIGITE O NUMERO A SER VERIFICADO: ")
    LEIA(numero)

    SE numero < 1 OU numero > 9 ENTAO
        ESCREVA("O NUMERO ", numero, " ESTA FORA DA FAIXA PERMITIDA DE VALORES")
    SENAO
        ESCREVA("O NUMERO ", numero, " ESTA NA FAIXA PERMITIDA DE VALORES")
    FIMSE
FIMALGORITMO
